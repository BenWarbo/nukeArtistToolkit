set N51774ec0 [stack 0]
 Transform {
  translate {{curve i} {slideTime i}}
  center {{Crop1.width/2 i} {Crop1.height/2 i}}
  black_outside false
  name Transform2
  xpos 510
  ypos 748
  disable {{!parent.scanlineenable i}}
  addUserKnob {20 Parameters l parameters}
  addUserKnob {7 waveLength}
  waveLength {{Input1.height+Crop1.height i}}
  addUserKnob {7 scanlinePosition R 0 200}
  scanlinePosition {{runningPosition i}}
  addUserKnob {12 slideTime}
  slideTime {0 {sawTooth(frame+(scanlinePosition+(scanlineamp*0.5))) i}}
  addUserKnob {7 sawTooth}
  sawTooth {{((1-((frame/scanlineamp)-(floor((frame/scanlineamp)))))*waveLength)-Crop1.height i}}
 }
